<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.timetable">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TimeTable">
        <activity
            android:name=".saturday"
            android:label="@string/saturday"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".friday"
            android:label="@string/friday"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".thursday"
            android:label="@string/thursday"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".wednesday"
            android:label="@string/wednesday"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".tuesday"
            android:label="@string/tuesday"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".monday"
            android:label="@string/monday"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>